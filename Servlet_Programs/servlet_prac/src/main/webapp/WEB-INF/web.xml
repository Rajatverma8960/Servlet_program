<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>abc</servlet-name>
    <servlet-class>com.kritter.servlet.Add_servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>abc</servlet-name>
    <url-pattern>/add</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>pqr</servlet-name>
    <servlet-class>com.kritter.servlet.sqServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>pqr</servlet-name>
    <url-pattern>/sq</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>HelloForm</servlet-name>
    <servlet-class>com.kritter.servlet.HelloForm</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>HelloForm</servlet-name>
    <url-pattern>/HelloForm</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>xyz</servlet-name>
    <servlet-class>com.kritter.servlet.Addition</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>xyz</servlet-name>
    <url-pattern>/Add</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>signup</servlet-name>
    <servlet-class>com.kritter.servlet.Signup</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>signup</servlet-name>
    <url-pattern>/signup</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.kritter.servlet.Login</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>profile</servlet-name>
    <servlet-class>com.kritter.servlet.Profile</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>profile</servlet-name>
    <url-pattern>/profile</url-pattern>
  </servlet-mapping>

</web-app>
